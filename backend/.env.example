# Server Configuration
PORT=3001
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# Logging
LOG_LEVEL=info

# External API Keys
GOOGLE_PAGESPEED_API_KEY=your_google_pagespeed_api_key_here
# Multiple API keys can be comma-separated for rotation
# GOOGLE_PAGESPEED_API_KEY=key1,key2,key3

# Schema.org Validator (optional, most functionality works without API key)
SCHEMA_VALIDATOR_API_KEY=

# External API Configuration
EXTERNAL_API_TIMEOUT=30000
EXTERNAL_API_RETRY_ATTEMPTS=3
GOOGLE_PAGESPEED_ENABLED=true
SCHEMA_VALIDATOR_ENABLED=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Analysis Configuration
MAX_CONCURRENT_ANALYSES=5
ANALYSIS_TIMEOUT_MS=30000
MAX_URLS_PER_REQUEST=10

# Security
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3000